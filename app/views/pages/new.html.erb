<%= form_for @user do |f| %>
  <%= f.label :firstname %>
  <%= f.text_field :firstname %><br />
 
  <%= f.label :lastname %>
  <%= f.text_field :lastname %><br />

  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %><br />
  
  <%= form_for @subject do |f| %>
  <%= f.label :subject %>
  <%= f.text_field :subject %><br />
<% end %>
 
 <%= f.submit "MATCH ME!" %>
<% end %>